class Solution {
    public int kthSmallest(TreeNode root, int k) {
      inorder(root);
      return arr.get(k-1);  
    
    }
    List<Integer>arr = new ArrayList<>();

    public void inorder(TreeNode root){
        if(root==null)
            return ;
        inorder(root.left);
        arr.add(root.val);
        inorder(root.right);
    }
}
